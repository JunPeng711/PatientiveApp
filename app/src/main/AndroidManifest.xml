        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MPSD2"
        tools:targetApi="31">
        <activity
            android:name=".ReportQuestion"
            android:exported="false" />
        <activity
            android:name=".Emergency"
            android:exported="false" />
        <activity
            android:name=".usermenu.ContactUs"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />

        <activity
            android:name=".doc_details.DoctorPsychological"
            android:exported="false" />
        <activity
            android:name=".CheckIn"
            android:exported="false" />
        <activity
            android:name=".usermenu.ChangePhone"
