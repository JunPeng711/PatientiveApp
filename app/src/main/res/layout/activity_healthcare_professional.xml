<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HealthcareProfessional">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="700dp"
        android:background="#CCC5C5"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/footBtn1"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:background="@drawable/home_icon"
            android:layout_marginStart="10dp"
            android:layout_gravity="bottom"/>

        <Button
            android:id="@+id/footBtn2"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="@drawable/report_icon"
            android:layout_marginStart="10dp"
            android:layout_gravity="bottom"/>

        <Button
            android:id="@+id/footBtn3"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:background="@drawable/checkin_icon"
            android:layout_marginStart="20dp"
            android:layout_gravity="bottom"/>

        <Button
            android:id="@+id/footBtn4"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:background="@drawable/appointment_icon"
            android:layout_marginStart="20dp"
            android:layout_gravity="bottom"/>

        <Button
            android:id="@+id/footBtn5"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:background="@drawable/health_education_icon"
            android:layout_marginStart="20dp"
            android:layout_gravity="bottom" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>